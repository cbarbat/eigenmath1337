pratt=1
-- integrals

trace = 1

check(integral(a) = a x)
check(integral(x) = 1/2 x^2)
check(integral(a x) = 1/2 a x^2)
check(integral(x^2) = 1/3 x^3)
check(integral(a x^2) = 1/3 a x^3)
check(integral(x^n) = x^(n + 1) / (n + 1))
check(integral(a x^n) = a x^(n + 1) / (n + 1))

-- check classifier false positives

check(integral(a^b,x) = a^b x)
check(integral(x^exp(y),x) = x^(exp(y) + 1) / (exp(y) + 1))
check(integral(x^log(y),x) = x^(log(y) + 1) / (log(y) + 1))
check(integral(x^sin(y),x) = x^(sin(y) + 1) / (sin(y) + 1))

-- x^n exp(a x + b)

f = exp(a x)
g = integral(f)
h = d(g)
check(f = h)

f = exp(a x + b)
g = integral(f)
h = d(g)
check(f = h)

f = x exp(a x)
g = integral(f)
h = d(g)
check(f = h)

f = x exp(a x + b)
g = integral(f)
h = d(g)
check(f = h)

f = x^2 exp(a x)
g = integral(f)
h = d(g)
check(f = h)

f = x^2 exp(a x + b)
g = integral(f)
h = d(g)
check(f = h)

f = x^3 exp(a x)
g = integral(f)
h = d(g)
check(f = h)

f = x^3 exp(a x + b)
g = integral(f)
h = d(g)
check(f = h)

-- x^n exp(a x^2 + b)

f = exp(a x^2)
g = integral(f)
h = d(g)
check(f = h)

f = exp(a x^2 + b)
g = integral(f)
h = d(g)
check(f = h)

f = x exp(a x^2)
g = integral(f)
h = d(g)
check(f = h)

f = x exp(a x^2 + b)
g = integral(f)
h = d(g)
check(f = h)

f = x^2 exp(a x^2)
g = integral(f)
h = d(g)
check(f = h)

f = x^2 exp(a x^2 + b)
g = integral(f)
h = d(g)
check(f = h)

f = x^3 exp(a x^2)
g = integral(f)
h = d(g)
check(f = h)

f = x^3 exp(a x^2 + b)
g = integral(f)
h = d(g)
check(f = h)

-- x^n log(a x + b)

f = log(a x)
g = integral(f)
h = d(g)
check(f = h)

f = log(a x + b)
g = integral(f)
h = d(g)
h = simplify(h)
check(f = h)

f = x log(a x)
g = integral(f)
h = d(g)
check(f = h)

f = x log(a x + b)
g = integral(f)
h = d(g)
h = simplify(h)
check(f = h)

f = x^2 log(a x)
g = integral(f)
h = d(g)
check(f = h)

f = x^2 log(a x + b)
g = integral(f)
h = d(g)
h = simplify(h)
check(f = h)

f = log(a x)^2
g = integral(f)
h = d(g)
check(f = h)

f = log(a x + b)^2
g = integral(f)
h = d(g)
h = simplify(h)
check(f = h)

f = log(a x) / x^2
g = integral(f)
h = d(g)
check(f = h)

f = log(a x + b) / x^2
g = integral(f)
h = d(g)
h = simplify(h)
check(f = h)

--

integral(x^2+x)
check(last = 1/3 x^3 + 1/2 x^2)

